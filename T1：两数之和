class solution
  def twoSum(self, nums, target):
    n = len(nums)
    for i in range(n):
      for j in range(i+1, n):
        if nums[i] + nums[j] == target:
          return[i, j]
    return[]

  def twoSum(self, nums, target):
    hashmap = dict()
    for i, num in enumerate(nums):
      if target - num in hashmap:
        return[hashmap[target - num], i]
      hashmap[nums[i]] = i
    return[]
